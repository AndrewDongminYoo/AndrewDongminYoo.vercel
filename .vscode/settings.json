{
    "cSpell.words": [
        "authorable",
        "bloglovin",
        "defaultlocale",
        "googlecalendar",
        "nextjs",
        "signoff",
        "unoptimized"
    ],
    "cSpell.ignoreRegExpList": [
        "CStyleComment"
    ],
    "cSpell.files": [
        "**/*",
        "!data/repos/*.json",
        "!data/posts/*.md",
        "!typings/*icon*.ts",
        "!components/repos/lang_colors.ts"
    ],
    "explorer.incrementalNaming": "disabled",
    "git.ignoreLimitWarning": true,
    "javascript.suggestionActions.enabled": false,
}